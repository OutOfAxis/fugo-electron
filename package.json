{
  "name": "fugo-cms-windows-player",
  "version": "1.7.0",
  "description": "Fugo CMS Windows Player",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -w"
  },
  "repository": "https://github.com/OutOfAxis/fugo-electron",
  "keywords": [
    "digital signage",
    "dooh"
  ],
  "author": "Fugo team",
  "devDependencies": {
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "ai.fugo.windowplayer",
    "mac": {
      "category": "public.app-category.business"
    },
    "appx": {
      "applicationId": "ai.fugo.windowsplayer",
      "backgroundColor": "#7048e8",
      "displayName": "Fugo Player"
    },
    "win": {
      
      "target": [
        {
          "target": "appx",
          "arch": "x64"
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "node-fetch": "^3.3.0"
  }
}
